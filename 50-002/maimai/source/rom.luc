module rom #(
    SELECT = 0
) (
    output rom1[32],
    output rom2[32],
    output rom3[32]
) {
    const ROM ={
        {32b10100100010001000100010001000101, 32b01010010001000100010001000100010, 
            32b00001001100100010001000100010000},
        {32b00101001000100010001000100010001, 32b10010010001000100010001000100010,
            32b01000100110010001000100010000100},
        {32b10001000100010001000100010001000, 32b01000100010001000100010001000100,
            32b00100010001000100010001000100011}
    }
    
    always {
        rom1 = ROM[SELECT][0]
        rom2 = ROM[SELECT][1]
        rom3 = ROM[SELECT][2]
    }
}